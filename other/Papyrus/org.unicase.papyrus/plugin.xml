<plugin>
   <extension-point id="service" name="Service" schema="schema/service.exsd"/>
   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://unicase.org/model/papyrus"
            class="org.unicase.papyrus.PapyrusPackage"
            genModel="model/papyrus.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.extension_parser">
      <parser
            type="papyrus"
            class="org.unicase.papyrus.util.PapyrusResourceFactoryImpl"/>
   </extension>
   <extension
         point="org.unicase.ecp.model.ecpModelPackage">
         <modelPackage
            modelPackage="http://unicase.org/model/papyrus"></modelPackage>
   </extension>
      <extension
            point="org.eclipse.ui.editors">
         <editor
               class="org.unicase.papyrus.custom.editors.UMLActivityDiagramEditor"
               default="false"
               extensions="Activity_diagram"
               id="org.unicase.papyrus.activity.UMLDiagramEditorID"
               name="ActivityDiagramEditor">
         </editor>
         <editor
               class="org.unicase.papyrus.custom.editors.UMLClassDiagramEditor"
               default="false"
               extensions="Class_diagram"
               id="org.unicase.papyrus.clazz.UMLDiagramEditorID"
               name="ClassDiagramEditor">
         </editor>
         <editor
               class="org.unicase.papyrus.custom.editors.UMLCommunicationDiagramEditor"
               default="false"
               extensions="Communication_diagram"
               id="org.unicase.papyrus.communication.UMLDiagramEditorID"
               name="CommunicationDiagramEditor">
         </editor>
         <editor
               class="org.unicase.papyrus.custom.editors.UMLCompositeDiagramEditor"
               default="false"
               extensions="Composite_diagram"
               id="org.unicase.papyrus.composite.UMLDiagramEditorID"
               name="CompositeDiagramEditor">
         </editor>
         <editor
               class="org.unicase.papyrus.custom.editors.UMLSequenceDiagramEditor"
               default="false"
               extensions="Sequence_diagram"
               id="org.unicase.papyrus.sequence.UMLDiagramEditorID"
               name="SequenceDiagramEditor">
         </editor>
         <editor
               class="org.unicase.papyrus.custom.editors.UMLStateMachineDiagramEditor"
               default="false"
               extensions="StateMachine_diagram"
               id="org.unicase.papyrus.statemachine.UMLDiagramEditorID"
               name="StateMachineDiagramEditor">
         </editor>
         <editor
               class="org.unicase.papyrus.custom.editors.UMLUseCaseDiagramEditor"
               default="false"
               extensions="UseCase_diagram"
               id="org.unicase.papyrus.usecase.UMLDiagramEditorID"
               name="UseCaseDiagramEditor">
         </editor>
      </extension>
      <extension
            point="org.eclipse.ui.contexts">
         <context
               id="org.unicase.papyrus.activity.diagramContext"
               name="UMLActivityContext"
               parentId="org.eclipse.gmf.runtime.diagram.ui.diagramContext">
         </context>
         <context
               id="org.unicase.papyrus.clazz.diagramContext"
               name="UMLClassContext"
               parentId="org.eclipse.gmf.runtime.diagram.ui.diagramContext">
         </context>
         <context
               id="org.unicase.papyrus.communication.diagramContext"
               name="UMLCommunicationContext"
               parentId="org.eclipse.gmf.runtime.diagram.ui.diagramContext">
         </context>
         <context
               id="org.unicase.papyrus.composite.diagramContext"
               name="UMLCompositeContext"
               parentId="org.eclipse.gmf.runtime.diagram.ui.diagramContext">
         </context>
         <context
               id="org.unicase.papyrus.sequence.diagramContext"
               name="UMLSequenceContext"
               parentId="org.eclipse.gmf.runtime.diagram.ui.diagramContext">
         </context>
         <context
               id="org.unicase.papyrus.statemachine.diagramContext"
               name="UMLStateMachineContext"
               parentId="org.eclipse.gmf.runtime.diagram.ui.diagramContext">
         </context>
         <context
               id="org.unicase.papyrus.usecase.diagramContext"
               name="UMLUseCaseContext"
               parentId="org.eclipse.gmf.runtime.diagram.ui.diagramContext">
         </context>
      </extension>
      <extension
         point="org.eclipse.emf.ecore.content_parser">
      <parser
            class="org.unicase.papyrus.custom.part.UMLModelDiagramResource"
            contentTypeIdentifier="UMLModel">
      </parser>
   </extension>
      <extension
            point="org.unicase.ui.common.modelelementopener">
         <ModelElementOpener
               class="org.unicase.papyrus.custom.part.UMLDiagramOpener"
               name="org.unicase.papyrus.UMLDiagramOpener">
         </ModelElementOpener>
      </extension>
      <extension
            point="org.unicase.papyrus.service">
         <serviceFactory
               classname="org.unicase.papyrus.custom.services.UnicaseEditingDomainServiceFactory"
               description="TransactionalEditingDomain"
               id="org.eclipse.emf.transaction.TransactionalEditingDomain"
               priority="1"
               startKind="startup">
         </serviceFactory>
         <serviceFactory
               classname="org.unicase.papyrus.custom.services.UnicasePageManagerServiceFactory"
               id="org.eclipse.papyrus.sasheditor.contentprovider.IPageMngr"
               priority="1"
               startKind="startup">
         </serviceFactory>
         <serviceFactory
            classname="org.unicase.papyrus.custom.services.UnicasePageIconRegistryServiceFactory"
            id="org.eclipse.papyrus.core.editorsfactory.IPageIconsRegistry"
            priority="1"
            startKind="startup">
      </serviceFactory>
      </extension>
   

</plugin>

         </includes>
      </viewerContentBinding>
      <viewerActionBinding viewerId="org.eclipse.ui.navigator.ProjectExplorer">
         <includes>
            <actionExtension pattern="org.unicase.wireframe.storyboard.navigatorActionProvider"/>
         </includes>
      </viewerActionBinding>
   </extension>

   <extension point="org.eclipse.ui.navigator.navigatorContent" id="navigator-content">
      <?gmfgen generated="true"?>
      <navigatorContent
            id="org.unicase.wireframe.storyboard.resourceContent" 
            name="%navigatorContentName" 
            priority="normal" 
            contentProvider="org.unicase.wireframe.storyboard.navigator.StoryboardNavigatorContentProvider" 
            labelProvider="org.unicase.wireframe.storyboard.navigator.StoryboardNavigatorLabelProvider"
            icon="icons/obj16/StoryboardDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="storyboard_diagram"/>
               </and>
               <instanceof value="org.unicase.wireframe.storyboard.navigator.StoryboardAbstractNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="org.unicase.wireframe.storyboard.navigator.StoryboardAbstractNavigatorItem"/>
            </or>
         </possibleChildren>
         <commonSorter 
               id="org.unicase.wireframe.storyboard.navigatorSorter" 
               class="org.unicase.wireframe.storyboard.navigator.StoryboardNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="storyboard_diagram"/>
                  </and>
                  <instanceof value="org.unicase.wireframe.storyboard.navigator.StoryboardAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            id="org.unicase.wireframe.storyboard.domainResourceContent" 
            name="%domainNavigatorContentName" 
            priority="normal" 
            contentProvider="org.unicase.wireframe.storyboard.navigator.StoryboardDomainNavigatorContentProvider" 
            labelProvider="org.unicase.wireframe.storyboard.navigator.StoryboardDomainNavigatorLabelProvider"
            icon="icons/obj16/StoryboardDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="storyboard"/>
               </and>
               <instanceof value="org.unicase.wireframe.storyboard.navigator.StoryboardDomainNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof value="org.unicase.wireframe.storyboard.navigator.StoryboardDomainNavigatorItem"/>
         </possibleChildren>
      </navigatorContent>
      <actionProvider
            id="org.unicase.wireframe.storyboard.navigatorActionProvider"
            class="org.unicase.wireframe.storyboard.navigator.StoryboardNavigatorActionProvider">
         <enablement>
            <or>
               <instanceof value="org.unicase.wireframe.storyboard.navigator.StoryboardAbstractNavigatorItem"/>
            </or>
         </enablement>
      </actionProvider>
   </extension>
   
	   <extension point="org.eclipse.ui.navigator.linkHelper" id="navigator-linkhelper">
      <?gmfgen generated="true"?>
      <linkHelper
            id="org.unicase.wireframe.storyboard.navigatorLinkHelper"
            class="org.unicase.wireframe.storyboard.navigator.StoryboardNavigatorLinkHelper">
         <editorInputEnablement>
            <and>
               <instanceof value="org.eclipse.emf.common.ui.URIEditorInput"/>
               <test property="org.unicase.wireframe.storyboard.isURIEditorInput"/>
            </and>
         </editorInputEnablement>
         <selectionEnablement>
            <instanceof value="org.unicase.wireframe.storyboard.navigator.StoryboardAbstractNavigatorItem"/>
         </selectionEnablement>
      </linkHelper>
   </extension>

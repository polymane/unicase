<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Sample Category"
            id="org.unicase.link.commands.category">
      </category>
      <command
            name="Sample Command"
            categoryId="org.unicase.link.commands.category"
            id="org.unicase.link.commands.sampleCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="org.unicase.link.commands.sampleCommand"
            class="org.unicase.link.handlers.UnicaseLinkHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.unicase.link.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
     
       <menuContribution
            locationURI="popup:org.unicase.ui.navigator.viewer?after=additions">
       
            <command
                  commandId="org.unicase.link.commands.sampleCommand"
                  label="Generate UNICASE link"
                  icon="icons/unicase_link.png"
                  mnemonic="U"
                  id="org.unicase.link.menus.sampleCommand">
            </command>
        
      </menuContribution>
      <menuContribution
      		locationURI="toolbar:org.unicase.ui.meeditor.MEEditorPage?after=org.unicase.ui.common.deleteModelElement">
            <command
                  commandId="org.unicase.link.commands.sampleCommand"
                  icon="icons/unicase_link.png"
                  tooltip="Generate UNICASE specific protocol link"
                  id="org.unicase.link.toolbars.sampleCommand">
            </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup></startup>
   </extension>
</plugin>

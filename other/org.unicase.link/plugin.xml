<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Sample Category"
            id="org.unicase.link.commands.category">
      </category>
      <command
            name="Sample Command"
            categoryId="org.unicase.link.commands.category"
            id="org.unicase.link.commands.sampleCommand">
      </command>
      <command
            categoryId="org.unicase.link.commands.category"
            id="org.unicase.link.commands.duplicateCommand"
            name="Duplicate Command">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="org.unicase.link.commands.sampleCommand"
            class="org.unicase.link.handlers.UnicaseLinkHandler">
      </handler>
      <handler
            class="org.unicase.link.handlers.DuplicateModelElementHandler"
            commandId="org.unicase.link.commands.duplicateCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.unicase.link.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <!--<menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="Sample Menu"
               mnemonic="M"
               id="org.unicase.link.menus.sampleMenu">
            <command
                  commandId="org.unicase.link.commands.sampleCommand"
                  mnemonic="S"
                  id="org.unicase.link.menus.sampleCommand">
            </command>
         </menu>
      </menuContribution>-->
      <menuContribution
      		locationURI="toolbar:org.unicase.ui.meeditor.MEEditorPage?after=additions">
            <command
                  commandId="org.unicase.link.commands.sampleCommand"
                  icon="icons/unicase_link.png"
                  tooltip="Generate unicase specific protocol link"
                  id="org.unicase.link.toolbars.sampleCommand">
            </command>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.unicase.ui.meeditor.MEEditorPage?after=additions">
         <command
               commandId="org.unicase.link.commands.duplicateCommand"
               icon="icons/unicase_link.png"
               id="org.unicase.link.toolbars.duplicateCommand"
               tooltip="Duplicate Action Item">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.unicase.ui.navigator.viewer?after=additions">
         <command
               commandId="org.unicase.link.commands.duplicateCommand"
               icon="icons/unicase_link.png"
               label="Duplicate Model Element"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup></startup>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
      		category="org.unicase.ui.common.unicase"
            class="org.unicase.link.preferences.UnicaseLinkPreferencePage"
            id="org.unicase.link.preferences.UnicaseLinkPreferencePage"
            name="Unicase URL">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.unicase.link.preferences.PreferenceInitializer">
      </initializer>
   </extension>

</plugin>

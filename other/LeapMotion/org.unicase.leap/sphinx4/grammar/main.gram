#JSGF V1.0;

/**
 * JSGF Grammar for Main Entry Point.
 */

grammar main;

import <class.attribute>;
import <class.operation>;
import <class.pattern>;
import <domain.view>;
import <tools.tool>;

public <main> = <tool> [<view> |
                        <domain.element> |
                        <pattern> |
                        <class.element> |
                        <attribute> |
                        <operation>];
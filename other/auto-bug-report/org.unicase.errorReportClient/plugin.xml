<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="org.unicase.errorreportclient.ViewPart1"
            id="org.unicase.errorReportClient.view2"
            name="name"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.unicase.errorreporting.reportHandlers">
      <ReportHandler
            class="org.unicase.errorreportclient.reporthandlers.TweeterReportHandler">
      </ReportHandler>
      <ReportHandler
            class="org.unicase.errorreportclient.reporthandlers.RSSReportHandler">
      </ReportHandler>
   </extension>
   <extension
         point="org.unicase.errorreporting.filters">
      <StackTraceFilter
            filter-value="org.unicase.errorReportClient.StackTraceFilter2">
      </StackTraceFilter>
      <StackTraceFilter
            filter-value="org.unicase.errorReportClient.StackTraceFilter3">
      </StackTraceFilter>
      <Severity
            severity-value="Error-Warning">
      </Severity>
   </extension>

</plugin>

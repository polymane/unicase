<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.unicase.errorreporting.filters">
      <StackTraceFilter
            filter-value="org.unicase.errorReportAnotherClient.StackTraceFilter5">
      </StackTraceFilter>
      <StackTraceFilter
            filter-value="org.unicase.errorReportAnotherClient.StackTraceFilter6">
      </StackTraceFilter>
      <StackTraceFilter
            filter-value="org.unicase.errorReportAnotherClient.StackTraceFilter7">
      </StackTraceFilter>
      <Severity
            severity-value="Error">
      </Severity>
   </extension>
   <extension
         point="org.unicase.errorreporting.reportHandlers">
      <DefaultReportHandler
            email="mailme@me">
      </DefaultReportHandler>
      <ReportHandler
            class="org.unicase.errorreportanotherclient.RSSHandler">
      </ReportHandler>
   </extension>

</plugin>

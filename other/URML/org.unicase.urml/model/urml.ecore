    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="GoalReference" eSuperTypes="#//UrmlModelElement platform:/plugin/org.unicase.metamodel/model/metamodel.ecore#//NonDomainElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="source" eType="#//goal/Goal"

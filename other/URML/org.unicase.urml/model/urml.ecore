          eType="#//goal/GoalReference" eOpposite="#//goal/GoalReference/target" eKeys="platform:/plugin/org.unicase.metamodel/model/metamodel.ecore#//IdentifiableElement/identifier"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="influencedGoals" upperBound="-1"

                 checkEnabled="false">
              <and>
                 <iterate
                       ifEmpty="false"
                       operator="and">
                    <instanceof
                          value="org.unicase.model.document.LeafSection">
                    </instanceof>
                 </iterate>
                 <count
                       value="1">
                 </count>
              </and>
           </visibleWhen>
        </command>
        <separator
              name="org.unicase.ui.navigator.createModelElementCommands"
              visible="true">
        </separator>
        <command
              commandId="org.unicase.ui.common.openDiagramWithMEEditor"
              label="Open with MEEditor"

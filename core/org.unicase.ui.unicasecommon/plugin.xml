                    </instanceof>
                 </iterate>
                 <count
                       value="1">
                 </count>
              </and>
           </visibleWhen>
           <command
                 commandId="org.unicase.ui.common.commands.addComment"
                 icon="icons/Comment.png"
                 label="Add Comment"
                 style="push">
              <visibleWhen
                    checkEnabled="false">
                 <and>
                    <iterate
                          ifEmpty="false"
                          operator="and">
                       <instanceof
                             value="org.unicase.metamodel.ModelElement">
                       </instanceof>
                    </iterate>
                    <count
                          value="1">
                    </count>
                 </and>
              </visibleWhen>
           </command>
           <command
                 commandId="org.unicase.ui.common.commands.annotateActionItem"
                 icon="icons/ActionItem.png"
                 label="Add Action Item"
                 style="push">
              <visibleWhen
                    checkEnabled="false">
                 <and>
                    <iterate
                          ifEmpty="false"
                          operator="and">
                       <reference
                             definitionId="org.unicase.ui.common.commands.directAnnotatable">
                       </reference>
                    </iterate>
                    <count
                          value="1">
                    </count>
                 </and>
              </visibleWhen>
           </command>
           <command
                 commandId="org.unicase.ui.common.commands.annotateIssue"
                 icon="icons/Issue.gif"
                 label="Add Issue"
                 style="push">
              <visibleWhen
                    checkEnabled="false">
                 <and>
                    <iterate
                          ifEmpty="false"
                          operator="and">
                       <reference
                             definitionId="org.unicase.ui.common.commands.directAnnotatable">
                       </reference>
                    </iterate>
                    <count
                          value="1">
                    </count>
                 </and>
              </visibleWhen>
           </command>
        </menu>
        <command
              commandId="org.unicase.ui.common.commands.subscribeToModelElement"
              icon="icons/subscribe.png"
              label="Subscribe"
              style="push">
           <visibleWhen
                 checkEnabled="false">
              <and>
                 <iterate
                       ifEmpty="false"
                       operator="and">
                    <instanceof
                          value="org.unicase.metamodel.ModelElement">
                    </instanceof>
                    <test
                          forcePluginActivation="true"
                          property="org.unicase.ui.common.commands.isNotSubscribed"
                          value="true">
                    </test>
                 </iterate>
                 <count
                       value="1">
                 </count>
              </and>
           </visibleWhen>
        </command>
        <command
              commandId="org.unicase.ui.common.commands.subscribeToModelElement"
              icon="icons/subscribe.png"
              label="Unsubscribe"
              style="push">
           <visibleWhen
                 checkEnabled="false">
              <and>
                 <iterate
                       ifEmpty="false"
                       operator="and">
                    <instanceof
                          value="org.unicase.metamodel.ModelElement">
                    </instanceof>
                    <test
                          forcePluginActivation="true"
                          property="org.unicase.ui.common.commands.isSubscribed"
                          value="true">
                    </test>
                 </iterate>
                 <count
                       value="1">
                 </count>
              </and>
           </visibleWhen>
        </command>
        <separator
              name="org.unicase.ui.common.commands.annotateActionItemSeperator"
              visible="true">
        </separator>
     </menuContribution>
     <menuContribution

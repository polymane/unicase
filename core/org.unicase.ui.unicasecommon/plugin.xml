                 icon="icons/ModelElement.png"
                 style="push"
                 tooltip="Shows open model element dialog">
           </command>
        </toolbar>
     </menuContribution>
     <menuContribution
           locationURI="popup:org.unicase.ui.navigator.viewer?after=additions">
        <command
              commandId="org.unicase.ui.navigator.followupMeeting"
              label="Follow-up meeting..."
              style="push">
           <visibleWhen
                 checkEnabled="false">
              <and>
                 <iterate
                       ifEmpty="false"
                       operator="and">
                    <instanceof
                          value="org.unicase.model.meeting.Meeting">
                    </instanceof>
                 </iterate>
                 <count
                       value="1">
                 </count>
              </and>
           </visibleWhen>
        </command>
        <dynamic
              class="org.unicase.ui.unicasecommon.navigator.commands.DynamicMECreationCommands"
              id="org.unicase.ui.navigator.dynamicMECreationCommands">
           <visibleWhen
                 checkEnabled="false">
              <and>
                 <iterate
                       ifEmpty="false"
                       operator="and">
                    <instanceof
                          value="org.unicase.model.document.LeafSection">
                    </instanceof>
                 </iterate>
                 <count
                       value="1">
                 </count>
              </and>
           </visibleWhen>
        </dynamic>
        <dynamic
              class="org.unicase.ui.unicasecommon.navigator.commands.DynamicContainmentCommands"
              id="org.unicase.ui.navigator.dynamicContainmentCommands">
           <visibleWhen
                 checkEnabled="false">
              <and>
                 <iterate
                       ifEmpty="false"
                       operator="and">
                    <reference
                          definitionId="org.unicase.ui.common.commands.modelElementNotSection">
                    </reference>
                 </iterate>
                 <count
                       value="1">
                 </count>
              </and>
           </visibleWhen>
        </dynamic>
        <command
              commandId="org.unicase.ui.navigator.newCompositeSection"
              icon="icons/CompositeSection.gif"
              label="New composite section"
              style="push">
           <visibleWhen
                 checkEnabled="false">
              <and>
                 <iterate
                       ifEmpty="false"
                       operator="and">
                    <instanceof
                          value="org.unicase.model.document.CompositeSection">
                    </instanceof>
                 </iterate>
                 <count
                       value="1">
                 </count>
              </and>
           </visibleWhen>
        </command>
        <command
              commandId="org.unicase.ui.navigator.newLeafSection"

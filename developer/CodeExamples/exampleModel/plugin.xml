<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http:///org/eclipse/example/library.ecore"
            class="library.LibraryPackage"
            genModel="model/library.genmodel"/>
   </extension>
   <extension
         point="org.unicase.metamodel.modelversion">
      <ModelVersion
            versionIdentifier="0">
      </ModelVersion>
   </extension>
   <extension
         point="org.eclipse.emf.validation.constraintProviders">
      <category
            id="library"
            mandatory="true"
            name="Library">
      </category>
      <constraintProvider
            cache="true"
            class="library.validation.BookConstraint"
            mode="Batch">
         <package
               namespaceUri="http:///org/eclipse/example/library.ecore">
         </package>
      </constraintProvider>
   </extension>

</plugin>

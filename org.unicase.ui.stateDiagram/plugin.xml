   <extension point="org.eclipse.ui.navigator.navigatorContent" id="navigator-content">
      <?gmfgen generated="true"?>
      <navigatorContent
            id="org.unicase.ui.stateDiagram.resourceContent" 
            name="%navigatorContentName" 
            priority="normal" 
            contentProvider="org.unicase.ui.stateDiagram.navigator.ModelNavigatorContentProvider" 
            labelProvider="org.unicase.ui.stateDiagram.navigator.ModelNavigatorLabelProvider"
            icon="icons/obj16/DiagramDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="state_diagram"/>
               </and>
               <instanceof value="org.unicase.ui.stateDiagram.navigator.ModelAbstractNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="org.unicase.ui.stateDiagram.navigator.ModelAbstractNavigatorItem"/>
            </or>
         </possibleChildren>
         <commonSorter 
               id="org.unicase.ui.stateDiagram.navigatorSorter" 
               class="org.unicase.ui.stateDiagram.navigator.ModelNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="state_diagram"/>
                  </and>
                  <instanceof value="org.unicase.ui.stateDiagram.navigator.ModelAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            id="org.unicase.ui.stateDiagram.domainResourceContent" 
            name="%domainNavigatorContentName" 
            priority="normal" 
            contentProvider="org.unicase.ui.stateDiagram.navigator.ModelDomainNavigatorContentProvider" 
            labelProvider="org.unicase.ui.stateDiagram.navigator.ModelDomainNavigatorLabelProvider"
            icon="icons/obj16/DiagramDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="model"/>
               </and>
               <instanceof value="org.unicase.ui.stateDiagram.navigator.ModelDomainNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof value="org.unicase.ui.stateDiagram.navigator.ModelDomainNavigatorItem"/>
         </possibleChildren>
      </navigatorContent>
      <actionProvider
            id="org.unicase.ui.stateDiagram.navigatorActionProvider"
            class="org.unicase.ui.stateDiagram.navigator.ModelNavigatorActionProvider">
         <enablement>
            <or>
               <instanceof value="org.unicase.ui.stateDiagram.navigator.ModelAbstractNavigatorItem"/>
            </or>
         </enablement>
      </actionProvider>

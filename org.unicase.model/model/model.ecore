<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="model"
    nsURI="http://unicase.org/model" nsPrefix="org.unicase.model">
  <eClassifiers xsi:type="ecore:EClass" name="ModelElement" abstract="true">
    <eOperations name="getProject" eType="#//Project"/>
    <eOperations name="addReader">
      <eParameters name="readerName" eType="#//organization/User"/>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://unicase.org/visibility">
        <details key="VisibilityValue" value="MANDATORY"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="description" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://unicase.org/visibility">
        <details key="VisibilityValue" value="VIEWWHENSET"/>
      </eAnnotations>
      <eAnnotations source="http://unicase.org/UIHints">
        <details key="type" value="TEXT_AREA"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="identifier" eType="#//ModelElementId"
        containment="true" eKeys="#//UniqueIdentifier/id"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="readerInfos" upperBound="-1"
        eType="#//ReaderInfo" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="actionItems" upperBound="-1"
        eType="#//task/ActionItem"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Project">
    <eOperations name="addModelElement">
      <eParameters name="modelElement" eType="#//ModelElement"/>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="modelElements" upperBound="-1"
        eType="#//ModelElement" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ModelElementId" eSuperTypes="#//UniqueIdentifier"/>
  <eClassifiers xsi:type="ecore:EClass" name="UniqueIdentifier" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        defaultValueLiteral="Default Value Literal&#x9;" iD="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ReaderInfo">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="date" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="readerId" eType="#//organization/OrgUnit"/>
  </eClassifiers>
  <eSubpackages name="organization" nsURI="http://unicase.org/model/organization"
      nsPrefix="org.unicase.model.organization">
    <eClassifiers xsi:type="ecore:EClass" name="User" eSuperTypes="#//organization/OrgUnit"/>
    <eClassifiers xsi:type="ecore:EClass" name="OrgUnit" eSuperTypes="#//ModelElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="orgId" eType="#//organization/OrgUnitId"
          containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="OrgUnitId" eSuperTypes="#//UniqueIdentifier"/>
    <eClassifiers xsi:type="ecore:EClass" name="Group" eSuperTypes="#//organization/OrgUnit"/>
  </eSubpackages>
  <eSubpackages name="task" nsURI="http://unicase.org/model/task" nsPrefix="org.unicase.model.task">
    <eClassifiers xsi:type="ecore:EClass" name="ActionItem" eSuperTypes="#//task/WorkItem">
      <eStructuralFeatures xsi:type="ecore:EReference" name="assignedTo" lowerBound="1"
          upperBound="-1" eType="#//organization/User"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="dueDate" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="done" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="estimate" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="WorkPackage" eSuperTypes="#//task/WorkItem">
      <eStructuralFeatures xsi:type="ecore:EReference" name="containedWorkItems" upperBound="-1"
          eType="#//task/WorkItem" containment="true" eOpposite="#//task/WorkItem/containingWorkpackage"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="WorkItem" abstract="true" interface="true"
        eSuperTypes="#//ModelElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="containingWorkpackage"
          eType="#//task/WorkPackage" eOpposite="#//task/WorkPackage/containedWorkItems"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="associatedChangePackages"
          upperBound="-1" eType="#//change/ModelChangePackage"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="diagram" nsURI="http://unicase.org/model/diagram" nsPrefix="org.unicase.model.diagram">
    <eClassifiers xsi:type="ecore:EClass" name="MEDiagram" eSuperTypes="#//ModelElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="elements" upperBound="-1"
          eType="#//ModelElement" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="gmfdiagram" eType="#//task/ActionItem"
          containment="true"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="classes" nsURI="http://unicase.org/model/classes" nsPrefix="org.unicase.model.classes">
    <eClassifiers xsi:type="ecore:EClass" name="Class" eSuperTypes="#//classes/PackageElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="participatedUseCases"
          upperBound="-1" eType="#//requirement/UseCase" eOpposite="#//requirement/UseCase/identifiedClasses"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="superClass" eType="#//classes/Class"
          eOpposite="#//classes/Class/subClasses"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="subClasses" upperBound="-1"
          eType="#//classes/Class" eOpposite="#//classes/Class/superClass"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Package" eSuperTypes="#//classes/PackageElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="containedPackageElements"
          upperBound="-1" eType="#//classes/PackageElement" containment="true" eOpposite="#//classes/PackageElement/parentPackage"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="PackageElement" abstract="true" eSuperTypes="#//ModelElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="parentPackage" eType="#//classes/Package"
          derived="true" eOpposite="#//classes/Package/containedPackageElements"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Association" eSuperTypes="#//ModelElement">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="directed" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="source" eType="#//classes/Class"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="#//classes/Class"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="#//classes/AssociationType"
          defaultValueLiteral=""/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="AssociationType" instanceTypeName="AssociationTypeInstance">
      <eLiterals name="Association" literal="ASSOCIATION"/>
      <eLiterals name="Aggregation" value="1" literal="AGGREGATION"/>
      <eLiterals name="Composition" value="2" literal="COMPOSITION"/>
      <eLiterals name="Depenedency" value="3" literal="DEPENDENCY"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="document" nsURI="http://unicase.org/model/document" nsPrefix="org.unicase.model.document">
    <eClassifiers xsi:type="ecore:EClass" name="LeafSection" eSuperTypes="#//document/Section">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="elementClass">
        <eGenericType eClassifier="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EJavaClass">
          <eTypeArguments>
            <eUpperBound eClassifier="#//ModelElement"/>
          </eTypeArguments>
        </eGenericType>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="modelElements" upperBound="-1"
          eType="#//ModelElement" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="CompositeSection" eSuperTypes="#//document/Section">
      <eStructuralFeatures xsi:type="ecore:EReference" name="subsections" upperBound="-1"
          eType="#//document/Section" containment="true" eOpposite="#//document/Section/parent"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Section" abstract="true" interface="true"
        eSuperTypes="#//ModelElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="parent" eType="#//document/CompositeSection"
          eOpposite="#//document/CompositeSection/subsections"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="requirement" nsURI="http://unicase.org/model/requirement" nsPrefix="org.unicase.model.requirement">
    <eClassifiers xsi:type="ecore:EClass" name="NonFunctionalRequirement" eSuperTypes="#//rationale/Criterion">
      <eStructuralFeatures xsi:type="ecore:EReference" name="restrictedScenarios"
          upperBound="-1" eType="#//requirement/Scenario" eOpposite="#//requirement/Scenario/nonFunctionalRequirements"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="restrictedUseCases" upperBound="-1"
          eType="#//requirement/UseCase" eOpposite="#//requirement/UseCase/nonFunctionalRequirements"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="FunctionalRequirement" eSuperTypes="#//ModelElement">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="reviewed" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="storyPoints" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="priority" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="refiningRequirements"
          upperBound="-1" eType="#//requirement/FunctionalRequirement" containment="true"
          eOpposite="#//requirement/FunctionalRequirement/refinedRequirement"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="refinedRequirement" eType="#//requirement/FunctionalRequirement"
          eOpposite="#//requirement/FunctionalRequirement/refiningRequirements"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="UseCase" eSuperTypes="#//ModelElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="steps" upperBound="-1"
          eType="#//requirement/Step" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="initiatingActor" eType="#//requirement/Actor"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="participatingActors"
          upperBound="-1" eType="#//requirement/Actor" eOpposite="#//requirement/Actor/participatedUseCases"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="scenarios" upperBound="-1"
          eType="#//requirement/Scenario" eOpposite="#//requirement/Scenario/instantiatedUseCases"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="functionalRequirements"
          upperBound="-1" eType="#//requirement/FunctionalRequirement"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="nonFunctionalRequirements"
          upperBound="-1" eType="#//requirement/NonFunctionalRequirement" eOpposite="#//requirement/NonFunctionalRequirement/restrictedUseCases"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="identifiedClasses" upperBound="-1"
          eType="#//classes/Class" eOpposite="#//classes/Class/participatedUseCases"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Scenario" eSuperTypes="#//ModelElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="steps" upperBound="-1"
          eType="#//requirement/Step" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="initiatingActorInstance"
          eType="#//requirement/ActorInstance"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="participatingActorInstances"
          upperBound="-1" eType="#//requirement/ActorInstance"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="instantiatedUseCases"
          upperBound="-1" eType="#//requirement/UseCase" eOpposite="#//requirement/UseCase/scenarios"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="functionalRequirements"
          upperBound="-1" eType="#//requirement/FunctionalRequirement"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="nonFunctionalRequirements"
          upperBound="-1" eType="#//requirement/NonFunctionalRequirement" eOpposite="#//requirement/NonFunctionalRequirement/restrictedScenarios"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Actor" eSuperTypes="#//ModelElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="initiatedUseCases" upperBound="-1"
          eType="#//requirement/UseCase"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="participatedUseCases"
          upperBound="-1" eType="#//requirement/UseCase" eOpposite="#//requirement/UseCase/participatingActors"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ActorInstance" eSuperTypes="#//ModelElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="initiatedScenarios" upperBound="-1"
          eType="#//requirement/Scenario"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="participatedScenarios"
          upperBound="-1" eType="#//requirement/Scenario"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Step" eSuperTypes="#//ModelElement">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="userStep" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="rationale" nsURI="http://unicase.org/model/rationale" nsPrefix="org.unicase.model.rationale">
    <eClassifiers xsi:type="ecore:EClass" name="Issue" eSuperTypes="#//ModelElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="proposals" upperBound="-1"
          eType="#//rationale/Proposal" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="solution" eType="#//rationale/Solution"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="criteria" upperBound="-1"
          eType="#//rationale/Criterion"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Proposal" eSuperTypes="#//ModelElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="assessments" upperBound="-1"
          eType="#//rationale/Assessment" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Solution" eSuperTypes="#//ModelElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="underlyingProposals"
          upperBound="-1" eType="#//rationale/Proposal"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Criterion" eSuperTypes="#//ModelElement"/>
    <eClassifiers xsi:type="ecore:EClass" name="Assessment" eSuperTypes="#//ModelElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="proposal" lowerBound="1"
          eType="#//rationale/Proposal"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="criterion" lowerBound="1"
          eType="#//rationale/Criterion"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="change" nsURI="http://unicase.org/model/change" nsPrefix="org.unicase.model.rationale">
    <eClassifiers xsi:type="ecore:EClass" name="ModelChangePackage" eSuperTypes="#//ModelElement">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="sourceVersion" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="targetVersion" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eClassifiers>
  </eSubpackages>
</ecore:EPackage>
